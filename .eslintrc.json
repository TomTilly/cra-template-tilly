{
  "extends": ["wesbos"],
  "rules": {
    "react/static-property-placement": 0,
    "react/prop-types": 0,
    "react/no-unused-state": 0,
    "jsx-a11y/label-has-associated-control": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ]
  }
}
